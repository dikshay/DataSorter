package com.dikshay.clasificacion.sorting;

import java.util.Vector;

public class SortingAlgorithms {


	public void InsertionSort(Vector<Integer> pElementsToBeSorted)
	{
		int key,i;
		for(int j = 1;j<pElementsToBeSorted.capacity();j++)
			{
				key = pElementsToBeSorted.get(j);
				i = j-1;

				while(i >= 0 && pElementsToBeSorted.get(i) > key)
					{	
						pElementsToBeSorted.add(i+1,pElementsToBeSorted.get(i));
						i = i-1;

					}

					
					
				gElementsToBeSorted.add(i+1,key);
			}
		System.out.println(gElementsToBeSorted);
	}
